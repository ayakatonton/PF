<div class="container mt-4">
  <div class="top">       
    <div class="row">
      <div class="col-md-10 mx-auto text-center ">
        <p class="mi">投稿写真一覧</p>
        <% if @photos.present? %>
          <% @photos.each do |photo| %>
            <%= link_to edit_admin_photo_path(photo) do %>
              <%= image_tag photo.image, size:"250x200" if photo.image.attached? %>
            <% end %>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>